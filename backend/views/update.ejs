<h1>Update Product</h1>
<form action="/update/<%=coinData._id%>" method="POST" enctype="multipart/form-data">
    <div>
        <label for="">Coin</label>
        <input type="name" name="name" value="<%=coinData.name%>">
    </div>
    <div>
        <label for="">Price</label>
        <input type="number" step="0.01" name="price" value="<%=coinData.price%>">
    </div>
    <div>
        <label for="">Coin Logo</label>
        <input type="file" name="coinlogo" value="" >
    </div>
    <div>
        <input type="submit" value="Submit">
    </div>
</form>
